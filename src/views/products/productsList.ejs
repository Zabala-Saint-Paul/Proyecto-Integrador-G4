<html lang="en">
<%- include('./../partials/head') %> 
<body>
	<main>
		<div class="divPrincipal">

			<% productsList.forEach(oneProduct => { %>
			<div class="divProductos">
		  		<h4 class="divtitulos" ><%= oneProduct.name %> </h4>
		  		<p class="divPrice"><strong>Precio:</strong> <%= oneProduct.price %> </p>
		  		<p class="divDescrip"><%= oneProduct.description %></p>
		  		<div>
		 	 		<a href="/productDetail/<%=oneProduct.id%>"><img class="imagLista" src="/images/<%= oneProduct.image %>" class="pdt-image"></a>
				</div>
			</div>  
		 
		  
		</div>
		<% }) %>
		<br>
	</main>
</body>
